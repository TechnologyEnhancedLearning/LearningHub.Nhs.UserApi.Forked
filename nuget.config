<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />		
	</packageSources>

	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="AutoMapper*" />
			<package pattern="Azure*" />
			<package pattern="coverlet.collector" />
			<package pattern="EntityFrameworkCore*" />
			<package pattern="FluentValidation*" />
		 	<package pattern="HtmlAgilityPack*" />
			<package pattern="IdentityServer4*" />
			<package pattern="IdentityModel*" />
			<package pattern="Microsoft*" />
			<package pattern="MockQueryable*" />
			<package pattern="Moq" />
			<package pattern="Newtonsoft*" />	
			<package pattern="NLog*" />
			<package pattern="StyleCop.Analyzers" />
			<package pattern="Swashbuckle*" />
			<package pattern="System*" />
			<package pattern="UAParser" />
			<package pattern="xunit*" />
		</packageSource>

		<packageSource key="LearningHubFeed">
			<package pattern="elfhHub.Nhs.*" />
			<package pattern="LearningHub.*" />
			<package pattern="NHSUKViewComponents.*"/>
		</packageSource>
	</packageSourceMapping>
</configuration>
